<h1>New Density</h1>

<%= form_tag(densities_url, method: 'post') do %>
  <div>
    <%= label_tag :zipcode %><br />
    <%= number_field_tag :zipcode %>
  </div>

  <div>
    <%= label_tag :densityofzip %><br />
    <%= text_field_tag :densityofzip %>
  </div>

  <div>
    <%= submit_tag "Create Density" %>
  </div>
<% end %>
