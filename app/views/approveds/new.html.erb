<h1>New Approved</h1>

<%= form_tag(approveds_url, method: 'post') do %>
  <div>
    <%= label_tag :zipcode %><br />
    <%= number_field_tag :zipcode %>
  </div>

  <div>
    <%= label_tag :status %><br />
    <%= check_box_tag :status %>
  </div>

  <div>
    <%= submit_tag "Create Approved" %>
  </div>
<% end %>
